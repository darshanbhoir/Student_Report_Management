@model IEnumerable<Student_Report_Management.Models.ViewModels.StudentReportViewModel>

@{
    ViewBag.Title = "SemesterwiseStudent";
}

<h2>SemesterwiseStudent</h2>


<h6>Enter Details</h6>
<form method="get" action="/MyAPI/SemesterwiseStudent">
    <label for="StudentName">Enter Student Name</label>
    <input type="text" name="StudentName" id="StudentName" />
    <br />
    <button type="submit">Search</button>
</form>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Report_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User_Score)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student_Mobile)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Semister_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Semister_Name)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Report_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User_Score)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student_Mobile)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Semister_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Semister_Name)
            </td>
        </tr>
    }
</table>


@Html.ActionLink("Back", "Index", "Home")