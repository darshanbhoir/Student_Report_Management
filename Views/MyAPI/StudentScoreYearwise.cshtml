@model IEnumerable<Student_Report_Management.Models.ViewModels.StudentReportViewModel>
@{
    ViewBag.Title = "StudentScoreYearwise";
}

<h2>Student Score Year wise</h2>

<h6>Enter Details</h6>
<form method="get" action="/MyAPI/StudentScoreYearwise">
    <label for="StudentId">Enter Student Id</label>
    <input type="number" name="StudentId" id="StudentId" />
    <br />
    <button type="submit">Search</button>
</form>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Student_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Report_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Semister_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Year_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User_Score)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Student_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Report_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Semister_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Year_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User_Score)
            </td>
        </tr>
    }
</table>


@Html.ActionLink("Back", "Index", "Home")